[collName] = true;\n  });\n\n  return function(type, data, info) {\n    if(type != \"db\" || (data && !collMap[data.coll])) {\n      return data\n    }\n\n    if(receiverType && receiverType != info.type)\n      return data;\n\n    if(name && name != info.name)\n      return data;\n\n    data.selector = \"[stripped]\";\n    return data;\n  };\n}"],"mappings":"AAAA;AACA;AACA;AACAA,MAAM,CAACC,cAAc,GAAG,SAASA,cAAcA,CAACC,YAAY,EAAEC,YAAY,EAAEC,IAAI,EAAE;EAChFF,YAAY,GAAIA,YAAY,IAAI,EAAE;EAElC,IAAIG,aAAa,GAAG,CAAC,CAAC;EACtBH,YAAY,CAACI,OAAO,CAAC,UAASC,IAAI,EAAE;IAClCF,aAAa,CAACE,IAAI,CAAC,GAAG,IAAI;EAC5B,CAAC,CAAC;EAEF,OAAO,UAAUA,IAAI,EAAEC,IAAI,EAAEC,IAAI,EAAE;IACjC,IAAGP,YAAY,CAACQ,MAAM,GAAG,CAAC,IAAI,CAACL,aAAa,CAACE,IAAI,CAAC,EAChD,OAAOC,IAAI;IAEb,IAAGL,YAAY,IAAIA,YAAY,IAAIM,IAAI,CAACF,IAAI,EAC1C,OAAOC,IAAI;IAEb,IAAGJ,IAAI,IAAIA,IAAI,IAAIK,IAAI,CAACL,IAAI,EAC1B,OAAOI,IAAI;IAEb,IAAGD,IAAI,IAAI,OAAO,EAAE;MAClBC,IAAI,CAACG,MAAM,GAAG,YAAY;IAC5B,CAAC,MAAM,IAAGJ,IAAI,IAAI,IAAI,EAAE;MACtBC,IAAI,CAACI,QAAQ,GAAG,YAAY;IAC9B,CAAC,MAAM,IAAGL,IAAI,IAAI,MAAM,EAAE;MACxBC,IAAI,CAACK,GAAG,GAAG,YAAY;IACzB,CAAC,MAAM,IAAGN,IAAI,IAAI,OAAO,EAAE;MACzB,CAAC,MAAM,EAAE,IAAI,EAAE,IAAI,EAAE,KAAK,EAAE,SAAS,CAAC,CAACD,OAAO,CAAC,UAASQ,IAAI,EAAE;QAC5D,IAAGN,IAAI,CAACM,IAAI,CAAC,EAAE;UACbN,IAAI,CAACM,IAAI,CAAC,GAAG,YAAY;QAC3B;MACF,CAAC,CAAC;IACJ;IAEA,OAAON,IAAI;EACb,CAAC;AACH,CAAC;;AAED;AACAR,MAAM,CAACe,cAAc,GAAG,SAASA,cAAcA,CAACC,cAAc,EAAEb,YAAY,EAAEC,IAAI,EAAE;EAClFY,cAAc,GAAGA,cAAc,IAAI,EAAE;EAErC,IAAIC,OAAO,GAAG,CAAC,CAAC;EAChBD,cAAc,CAACV,OAAO,CAAC,UAASY,QAAQ,EAAE;IACxCD,OAAO,CAACC,QAAQ,CAAC,GAAG,IAAI;EAC1B,CAAC,CAAC;EAEF,OAAO,UAASX,IAAI,EAAEC,IAAI,EAAEC,IAAI,EAAE;IAChC,IAAGF,IAAI,IAAI,IAAI,IAAKC,IAAI,IAAI,CAACS,OAAO,CAACT,IAAI,CAACW,IAAI,CAAE,EAAE;MAChD,OAAOX,IAAI;IACb;IAEA,IAAGL,YAAY,IAAIA,YAAY,IAAIM,IAAI,CAACF,IAAI,EAC1C,OAAOC,IAAI;IAEb,IAAGJ,IAAI,IAAIA,IAAI,IAAIK,IAAI,CAACL,IAAI,EAC1B,OAAOI,IAAI;IAEbA,IAAI,CAACI,QAAQ,GAAG,YAAY;IAC5B,OAAOJ,IAAI;EACb,CAAC;AACH,CAAC"},"sourceType":"module","ex